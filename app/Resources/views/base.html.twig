<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% stylesheets %}
    	<!--<link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />-->
        {% include 'BraincraftedBootstrapBundle::_stylesheets.html.twig' %}
        {% endstylesheets %}
        
        {% javascripts '@AndresTestBundle/Resources/public/js/*' filter='yui_js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        {% endjavascripts %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
        <h1>h1 Heading</h1>
        <h2>h2 Heading</h2>
        <h3>h3 Heading</h3>
        <h4>h4 Heading</h4>
        <h5>h5 Heading</h5>
        <h6>h6 Heading</h6>
        {% endblock %}
        {% block footer %}
            <br />
            <div id="footer">
                <a href="{{ path('andres_test_home') }}">Home</a>
                <a href="{{ path('andres_test_content') }}">Content</a>
                <a href="history">History</a>
                <a href="{{ path('andres_crear_menu') }}">Crear nuevo dato</a>
                <a href="{{ path('andres_mostrar_menu') }}">Mostrar datos</a>
            </div>
        {% endblock %}
        {% block javascripts %}

        {% endblock %}
        {% include 'BraincraftedBootstrapBundle::_javascripts.html.twig' %}
    </body>
</html>
